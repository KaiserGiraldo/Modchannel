<div class="appealForm">
  <table>
    <tbody>
      <tr>
        <th>ID</th>
        <td class="idLabel"></td>
      </tr>
      <tr class="asnPanel">
        <th>ASN</th>
        <td class="asnLabel"></td>
      </tr>
      <tr>
        <th>IP</th>
        <td class="targetLabel"></td>
      </tr>
      <tr id="reasonPanel">
        <th>Reason</th>
        <td class="reasonLabel"></td>
      </tr>
      <tr class="boardPanel">
        <th>Board</th>
        <td class="boardLabel"></td>
      </tr>
      <tr id="expirationPanel">
        <th>Expiration</th>
        <td class="expirationLabel"></td>
      </tr>
      <tr>
        <th>Applied by</th>
        <td class="appliedByLabel"></td>
      </tr>
      <tr class="appealPanel">
        <th>Appeal</th>
        <td class="appealLabel"></td>
      </tr>
      <tr>
        <th>Links</th>
        <td>
          <a class="postHistoryLink">[Post history]</a>
          <a class="offenseRecordLink">[Offense record]</a>
          <a class="fileHistoryLink">[File history]</a>
        </td>
      </tr>
      <tr>
        <th>Action</th>
        <td>
          <form style="display:inline" action="/liftBan.js" method="post" enctype="multipart/form-data">
