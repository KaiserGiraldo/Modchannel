            <input type="hidden" class="liftIdentifier" name="banId">
          </form>
          <form style="display:inline" action="/denyAppeal.js" method="post" enctype="multipart/form-data">
            <button type="submit" class="denyFormButton denyForm">Deny appeal</button>
            <input type="hidden" name="banId" class="denyIdentifier">
          </form>
